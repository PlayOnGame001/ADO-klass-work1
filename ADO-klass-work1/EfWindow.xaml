<Window x:Class="ADO_klass_work1.EfWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO_klass_work1"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="EfWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Label Content="Manager"/>
            <Label Content="--" x:Name="ManagersCountLabel"/>
            <Label Content="Product"/>
            <Label Content="--" x:Name="ProductCountLabel"/>
            <Label Content="Sale"/>
            <Label Content="--" x:Name="SaleCountLabel"/>
        </StackPanel>
    </Grid>
</Window>
