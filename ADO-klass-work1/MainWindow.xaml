<Window x:Class="ADO_klass_work1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO_klass_work1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400"
        Background="Bisque">
    <StackPanel>
        <Label> </Label>
        <Button x:Name="IntoButton" Content="Вcтуп" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="Button_Click_1"/>
        <Button x:Name="AuthButton"  Content="Аутентификация/Регистрация" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="AuthButton_Click"/>
        <Button x:Name="CrudButton"  Content="CRUD" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="CrudButton_Click"/>
        <Button x:Name="EfButton"  Content="EfWindow" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="EfButton_Click"/>
        <Button x:Name="EfCrudButton"  Content="Ef Crud" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="EfCrudButton_Click"/>
        <Button x:Name="PracticeWindow"  Content="Ef Crud" FontSize="18" FontWeight="DemiBold" Margin="20 10 20 10" Padding="5" Click="PracticeWindow_Click"/>
    </StackPanel>
</Window>
